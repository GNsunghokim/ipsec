connect 192.168.100.254
$vmid = create core: 1 memory: 0x1000000 storage: 0x1000000 nic: mac: 0 dev: eth0 ibuf: 1024 obuf: 1024 iband: 1000000000 oband: 1000000000 hpad: 128 tpad: 128 pool: 0x400000 nic: mac: 0 dev: eth0 ibuf: 1024 obuf: 1024 iband: 1000000000 oband: 1000000000 hpad: 128 tpad: 128 pool: 0x400000 args: hello world
upload $vmid main
start $vmid

#Manage IP
stdin $vmid 0 "ip add eth0 192.168.100.210"
stdin $vmid 0 "ip add eth0 192.168.100.211"

#Manage SP
stdin $vmid 0 "sp add eth0 -p any -s 192.168.100.0/24:0 -d 192.168.100.0/24:0 -a ipsec/out -i 0 -o eth1"
stdin $vmid 0 "sp add eth0 -p any -s 0.0.0.0/32:0 -d -0.0.0.0/32:0 -a bypass/out -i 1 -o eth1"

stdin $vmid 0 "sp add eth1 -p any -s 192.168.101.1/32:80 -d 192.168.100.0/24:0 -a ipsec/in -i 0 -o eth0"
stdin $vmid 0 "sp add eth1 -p any -s 0.0.0.0/32:0 -d -0.0.0.0/32:0 -a bypass/in -i 1 -o eth0"

#Manage Content
stdin $vmid 0 "content add eth0 0 -m tunnel 10.0.0.1-10.0.0.10 -E des_cbc -A hmac_md5 -i 0"
stdin $vmid 0 "content add eth0 0 -m transport -A hmac_md5 -i 1"

stdin $vmid 0 "content add eth1 0 -m tunnel 10.0.0.10-10.0.0.1 -E des_cbc -A hmac_md5 -i 0"
stdin $vmid 0 "content add eth1 0 -m transport -A hmac_md5 -i 1"

#Manage SA
stdin $vmid 0 "sa add eth0 -m tunnel -s 10.0.0.1/32:0 -d 10.0.0.10/32:0 -E des_cbc 0x782FE28489CFD6A8 0 -A hmac_md5 0xf11 0"
stdin $vmid 0 "sa add eth0 -m transport -s 10.0.0.1/32:0 -d 10.0.0.10/32:0 -A hmac_md5 0xf11 0"
stdin $vmid 0 "sp list"
stdin $vmid 0 "sa list"
