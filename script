connect 192.168.100.254
$vmid = create core: 1 memory: 0x1000000 storage: 0x1000000 nic: mac: 0 dev: eth0 ibuf: 1024 obuf: 1024 iband: 1000000000 oband: 1000000000 pool: 0x400000 nic: mac: 0 dev: eth0 ibuf: 1024 obuf: 1024 iband: 1000000000 oband: 1000000000 pool: 0x400000 args: hello world
upload $vmid main
start $vmid

#Manage IP
stdin $vmid 0 "ip add eth0 192.168.100.210"
stdin $vmid 0 "ip add eth0 192.168.100.211"

#Manage SP
stdin $vmid 0 "sp add eth0 -p tcp -s 192.168.100.0/24:0 -d 192.168.101.1/32:80 -a ipsec/out -i 0 -o eth1"
stdin $vmid 0 "sp add eth0 -p any -s 0.0.0.0/24:0 -d -0.0.0.0/32:0 -a bypass/out -i 1 -o eth1"

#Manage Content
stdin $vmid 0 "content add eth0 0 -m transport -E des_cbc -A hmac_md5 -i 0"
stdin $vmid 0 "content add eth0 0 -m tunnel 10.0.0.1-10.0.0.10 -E des_cbc -A hmac_md5 -i 0"

#Manage SA
stdin $vmid 0 "sa add eth0 -s 192.168.100.0/24:0 -d 192.168.101.1/32:80 -E blowfish_cbc 0x165434ffcf0f0909001144 0 -A hmac_md5 0xf11 0"
stdin $vmid 0 "sa add eth0 -s 192.168.100.0/24:0 -d 192.168.101.1/32:80 -A hmac_md5 0xf11 0"
stdin $vmid 0 "sa add eth0 -s 192.168.100.0/24:0 -d 192.168.101.1/32:80"
stdin $vmid 0 "sp list"
stdin $vmid 0 "sa list"
